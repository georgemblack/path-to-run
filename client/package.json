  "description": "Front-end code for Path to Run",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.html",
    "build:watch": "parcel watch src/index.html",
    "serve": "http-server dist/",
    "lint": "eslint src/**.{js,vue} src/**/*.{js,vue}",
