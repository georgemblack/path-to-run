    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "http-server": "^0.11.1",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.17.3",
    "vue-template-compiler": "^2.6.10"
  }
}
